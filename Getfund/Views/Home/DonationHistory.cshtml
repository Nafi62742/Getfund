@model List<Getfund.Models.Donation>

@{
    ViewBag.Title = "DonationHistory";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">PId</th>
            <th scope="col">DonateTime</th>
            <th scope="col">Project Describtion</th>
            <th scope="col">Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.DonationId</th>
                <td>@item.PId</td>
                <td>@item.DonateTime</td>
                <td>@item.DonateDes</td>
                <td>@item.Amount</td>
            </tr>
        }
    </tbody>
</table>
